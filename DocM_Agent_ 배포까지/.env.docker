# Docker Compose Environment Variables
# Copy this file to .env and fill in your actual values

# =============================================================================
# Supabase Configuration (REQUIRED)
# =============================================================================
SUPABASE_URL=https://ollzcexpyjageofhmysa.supabase.co
SUPABASE_ANON_KEY=your_supabase_anon_key_here

# =============================================================================
# Serper API Configuration (REQUIRED for web search)
# =============================================================================
SERPER_API_KEY=your_serper_api_key_here

# =============================================================================
# vLLM Model Configuration
# =============================================================================
# Path to your fine-tuned model on the HOST machine
VLLM_MODEL_PATH=/home/lys/Desktop/qwen3_4b_ft/final_merged_model

# Model name served by vLLM
VLLM_SERVED_MODEL_NAME=qwen3-4b-ft

# GPU memory utilization (0.0-1.0, reduce if OOM errors occur)
VLLM_GPU_MEMORY_UTILIZATION=0.75

# Max number of concurrent sequences (reduce if OOM errors occur)
VLLM_MAX_NUM_SEQS=128

# =============================================================================
# LLM Configuration
# =============================================================================
VLLM_MODEL_NAME=qwen3-4b-ft
VLLM_API_KEY=EMPTY
VLLM_TEMPERATURE=0.2
VLLM_MAX_TOKENS=1024

# =============================================================================
# Embedding Model Configuration
# =============================================================================
EMBEDDER_MODEL=intfloat/multilingual-e5-large-instruct
EMBED_DIM=1024

# =============================================================================
# RAG Configuration (Advanced)
# =============================================================================
SIMILARITY_THRESHOLD=0.72
RAG_TOP_K=8
DOC_ONLY_THRESHOLD=0.85
WEB_FALLBACK_THRESHOLD=0.65
QUESTION_COVERAGE_THRESHOLD=0.6
MAX_CONTEXT_CHUNKS=6
MAX_FINAL_PROMPT_CHUNKS=6
RAG_KEYWORD_BOOST=0.03
